<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="com.example.music.model.User" alias="User"/>
		<typeAlias type="com.example.music.model.PlayList" alias="PlayList"/>
		<typeAlias type="com.example.music.model.Video" alias="Video"/>
		<typeAlias type="com.example.music.model.PlaylistVideo" alias="PlaylistVideo"/>
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/user.xml"/>
		<mapper resource="mybatis/playlist.xml"/>
		<mapper resource="mybatis/video.xml"/>
		<mapper resource="mybatis/playlistVideo.xml"/>
	</mappers>

</configuration>